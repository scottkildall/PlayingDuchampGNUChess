msgstr "Sem suporte ao comando \"show eval/score\" no momento.\n"

#: src/frontend/cmd.cc:939
#, c-format
msgid "Command 'show pin' is currently not supported.\n"
msgstr "Sem suporte ao comando \"show pin\" no momento.\n"

